{
  "name": "flaskfarm-dev",
  "dockerComposeFile": [
    "../docker/docker-compose.yaml"
  ],
  "service": "flaskfarm-dev",
  "workspaceFolder": "/flaskfarm-dev",
  "forwardPorts": [9999],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.black-formatter"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "[python]": {
          "editor.defaultFormatter": "ms-python.black-formatter"
        }
      }
    }
  }
}